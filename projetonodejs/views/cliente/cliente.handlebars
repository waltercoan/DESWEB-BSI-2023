<h1>Clientes</h1>
<a href="./clientes/novo" class="btn btn-primary">Novo</a>
<hr>
<table class="table" id="clientes">
    <!-- Títulos da tabela -->
    <thead> 
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>Endereço</th>
            <th>Sexo</th>
            <th>Telefone</th>
            <th></th>
        </tr>    
    </thead>
    <!-- Conteúdo da tabela -->
    <tbody>
        {{#each listaclientes}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.nome}}</td>
            <td>{{this.endereco}}</td>
            <td>{{this.sexo}}</td>
            <td>{{this.telefone}}</td>
            <td><a href="/clientes/alterar/{{this.id}}" 
            class="btn btn-warning">Alterar</a>
                <button class="btn btn-danger js-delete"
                        data-id="{{this.id}}">Excluir</button>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>

<script src="/js/jquery.min.js"></script>
<script src="/public/site.js"></script>